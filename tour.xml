	<!-- DEBUG PURPOSES -->
	<action name="print">
		showlog();
		trace(" ",%1);
	</action>
	
	<events onkeydown="debugaction();" if="1"/>
	
	<action name="debugaction" if="1" >
		if(keycode == 89, <!-- Y KEY -->
			print(y);
		);
		if(keycode == 85, <!-- U KEY -->
		);
		if(keycode == 72, <!-- H KEY -->
		);
	</action>
